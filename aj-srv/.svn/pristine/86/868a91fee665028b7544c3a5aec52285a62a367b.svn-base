package com.frame.ifpr.exception;

/**
 * 接口发布异常
 * 
 * @author lishun
 * 
 */
public class PublicException extends BaseException {

	private static final long serialVersionUID = 1L;

	public PublicException() {
		super();
	}

	public PublicException(String errMessage, Throwable e) {
		super(errMessage, e);
	}

	public PublicException(String errMessage) {
		super(errMessage);
	}

	public PublicException(Throwable errMessage) {
		super(errMessage);
	}

}
