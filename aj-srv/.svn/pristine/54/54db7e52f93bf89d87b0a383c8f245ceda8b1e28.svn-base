@charset "utf-8";
/* CSS Document */
body,form,ul,ol,li,p,h1,h2,h3,h4,h5,h6,dl,dt,dd,table,fieldset{margin:0; padding:0;}
body,input,select,textarea{color:#706e6e; font:12px/1.5 "宋体",Tahoma,Arial,Helvetica,STHeiti; vertical-align:middle; }
img{border:0; vertical-align:middle;}
ul,ol,li{list-style-type:none;}
a{color:#666; outline:none; text-decoration:none;}
a:hover{text-decoration:none;}
.clear{ clear:both;}

/* header */
.sme_header{ width:auto; height:60px; background:url(../../images/201202/sme__02.jpg) repeat-x; clear:both; margin:0 auto; }
.sme_top{  height:60px; overflow:hidden; margin:0 auto; clear:both; background:url(../../images/201202/bg65.jpg) right top scroll no-repeat;}
.sme_logo{margin:6px 0px 0px 30px; width:180px; height:45px; background:url(../../images/201202/sme__051.jpg) no-repeat; display:inline;}
.sme_logo a{ width:180px; height:45px; display:block;}
.sme_top{ }
.sme_logo{ float:left;}
.sme_top h1{ color:#666; font-size:18px; float:left; font-family:"微软雅黑", "宋体", Arial; font-weight:100; height:60px; line-height:60px; margin-left:0px; width:430px;}
.sme_top h2{ display:block; float:right; font-size:12px; font-family:"宋体"; width:300px; font-weight:normal; line-height:80px;}
.sme_top h2 a{ color:#1170bb;}
.sme_top h2 a:hover{ text-decoration:underline;}




/* sem_nav */
.sme_nav{ width:auto; border:#5991c9 solid 1px; height:39px; margin:0 auto; margin-top:1px; clear:both;}
.sme_navbg{  height:30px; margin:0px; padding:0px; margin-left:0px; background:url(../../images/201202/manage_03.jpg) repeat-x;}
.sme_navbg h1{ font-family:"微软雅黑", "宋体", Arial; font-size:16px; color:#FFF; line-height:28px; font-weight:normal;height:30px; display:block; float:left; background:url(../../images/201202/manage__03h.gif) no-repeat; background-position: 10px center; padding-left:35px;}
.sme_navbg h1 img{ vertical-align:middle;padding:0px 5px 7px 10px; }

.sme_navmin{ float:left; color:#808080; padding:0 20px; height:25px; margin-top:2px;-moz-border-radius:5px;-moz-border-radius: 5px;-khtml-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; background:#e7f4f9; line-height:26px; _line-height:23px; margin-left:40px;  _height:23px;}
.sme_navmin a{ color:#1e5885;}
.sme_navmin a:hover{ text-decoration:underline;}

/* sem_main */
.sme_main{ width:183px; clear:both; margin:0 auto; margin-top:4px;}
.sme_mainl{height:200px; width:165px; margin-top:1px;width:175px;-moz-border-radius:0 0 5px 5px;-moz-border-radius: 5px;-khtml-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; border:#dedede solid 6px; float:left; background:#f7f7f7;}
.sme_mainr{ float:left; width:1072px; margin-left:7px; margin-top:4px;}
.sme_mainrm{height:auto; width:1072px;}
.sme_search{ width:auto; height:45px; border:#d0d0d0 solid 1px;-moz-border-radius:0 0 5px 5px;-moz-border-radius: 5px;-khtml-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; background:#f7f7f7;  clear:both;}
.sme_search div{  float:left; padding-top:10px;}
.sme_search div input{ height:22px; padding:0; margin:0px; padding:0px;}
.sme_search div select{ width:130px; height:22px; line-height:22px;}
.sme_search1{ width:156px; margin:0px; padding:0px; margin-bottom:3px; -moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;height: 22px;*height: 22px;}
.sme_search2{ margin:0px 58px 0px 40px;}
.sme_search2 input{ width:100px; -moz-box-sizing: border-box;-webkit-box-sizing: border-box;box-sizing: border-box;height: 21px;*height: 16px; margin:0px 5px;}
#sme_search3{ background:url(../../images/201202/manage__07.jpg) no-repeat; width:67px; height:24px; border:none;margin-left:10px;cursor:pointer; }

.sme_newlist{ _height:99%; min-height:99%;-moz-border-radius:0 0 5px 5px;-moz-border-radius: 5px;-khtml-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 3px; border:#dedede solid 6px; width:80%; margin-top:4px; background:#FFF; _padding:6px;}
.sme_newinput{ height:28px; margin-top:4px; padding-left:6px; position:relative;  }
.sme_newinput input{ background:url(../../images/201202/manage__03.gif) no-repeat; border:none; width:98px; height:28px; cursor:pointer;}
.sme_newmes{ padding-left:6px; height:30px; line-height:30px; width:99%; overflow:hidden;}
.sme_newmes a{ color:#1170bb; padding:0px 5px;CURSOR: hand;}
.sme_newmes a:hover{ text-decoration:underline;}

.sme_newul{ width:1042px; margin-left:6px; background:#dadada;}
.sme_newul tr{ background:#FFF;}
.sme_newul td{ height:40px; line-height:40px; text-align:center;}
.sme_newul .sme_news1{ width:61px; }
.sme_newul .sme_news1 input{ vertical-align:middle; margin-bottom:8px;}
.sme_newul .sme_news11{ width:61px; }
.sme_newul .sme_news11 input{ vertical-align:middle; margin-right:25px;}
.sme_newul .sme_news2{width:385px; }
.sme_newul .sme_news22{ background:#fffbed;width:385px; height:40px; line-height:39px; text-align:left;}
.sme_newul .sme_news22 img{ margin:0px 5px 3px 30px;}
.sme_newul .sme_news3{width:88px;}
.sme_newul .sme_news4{width:73px;}
.sme_newul .sme_news5{width:75px;}
.sme_newul .sme_news6{width:165px;}
.sme_newul .sme_news7{width:188px;}
.sme_newsfist{ background:#dcf0ff; }
.sme_news7 a img{ margin:13px 10px 13px 10px;}

.sme_page{ width:1042px; height:45px; margin-left:6px; line-height:45px;color:#000;}
.sme_pager{ float:right;}


/* footer */
.sme_footer{ height:40px; width:auto; border:#efefef solid 1px; background:#fafafa; margin:0 auto; margin-top:0px; line-height:40px; }
.sme_footer .sme_about{ float:left; margin-left:20px;}
.sme_footer .sme_about a{ margin:0px 5px;}
.sme_footer .sme_copyright{ float:right; margin-right:20px;}

/* sme_mainl */
.sme_mainltit{ height:30px; background:#8ec5ef; width:auto; color:#FFF; font-size:14px;  font-weight:bold;}
.ltjg_lefttop{ background:url(../../images/201202/lefttop__02.gif) no-repeat; color:#FFF; padding-left:42px; height:26px; line-height:26px; width:180px;}


/*sz-new*/
.sme_navmin, .sme_search, .sme_search01{behavior: url(css/201202/ie-css3.htc); position:relative;  z-index:1;}
.sme_mainl {behavior: url(css/201202/ie-css3.htc); z-index:2; position:relative;}
.sme_copyright  span{font-family:Arial, Helvetica, sans-serif; font-size:12px; margin-right:5px;}
.sme_newlist{behavior: url(css/201202/ie-css3.htc); position:relative;  z-index:0;}
.sme_nav_1{border:#5991c9 solid 1px; height:30px; margin:5px 3px 0 ;  clear:both;}

/*通用状态输入框*/
.geinputleft{background:url(../../images/201202/ginputleft.gif) no-repeat left top; padding-left:5px; width:150px; display:inline-block;}
.geinputright{background:url(../../images/201202/ginputright.gif) no-repeat right top; padding-right:5px;  display:inline-block;}
.geinputmid{background:url(../../images/201202/ginputmid.gif) repeat-x; height:24px;  display:block;}
.geinputmid input{  width:145px; border:none; background:none; line-height:24px;  height:24px;}
.sme_search01{ width:1030px;  border:#d0d0d0 solid 1px; -moz-border-radius: 5px;-khtml-border-radius: 5px;-webkit-border-radius: 5px;border-radius: 5px; background:#f7f7f7;  clear:both; padding:10px 20px;}


/*cms部分*/
.top_catalog{border:#dddddd 1px solid; margin:0 auto; margin-top:1px; width:auto; background:url(../../images/201202/cmstopbg.gif) repeat-x scroll; height:30px; _height:29px; -moz-border-radius: 5px 5px 0px 0px;-khtml-border-radius: 5px 5px 0px 0px;-webkit-border-radius: 5px 5px 0px 0px;border-radius: 5px 5px 0px 0px; behavior: url(css/201202/ie-css3.htc); position:relative;  z-index:1;}
.piaozuo{font-size:13px; font-weight:bold; line-height:30px; padding:0 20px; float:left; width:90px;}
.piaoyou{float:left; }

.top_catalog_left { width: 20px; float:left; padding-right:20px;}
.top_catalog_left  a{background: url(../../images/201202/left_Arrowg.gif) no-repeat scroll left top  transparent; display: inline-block; height: 0 !important; overflow: hidden; z-index: 500;padding-top:15px;position: relative; top: 8px;}
.top_catalog_left  a:hover{background: url(../../images/201202/left_Arrow.gif) no-repeat scroll left top  transparent;}
.top_catalog_mid{ width:950px; text-wrap:normal; white-space:nowrap; overflow:hidden;  float:left; }
.top_catalog_mid li{display:inline; list-style:none;  font-size:13px; font-weight:bold;}

.top_catalog_mid a{ background:url(../../images/201202/cmstopbg.gif) repeat-x scroll; padding:0 20px; border-left:solid 1px #dddddd ; _line-height:30px; line-height:29px; display:inline-block; }
.top_catalog_mid a:hover{color:#1170BB;}
#seleclt{ background:url(../../images/201202/cmstopbgo.gif) repeat-x scroll;}
.seleclt123{ background:url(../../images/201202/cmstopbgo.gif) repeat-x scroll;}

.top_catalog_right{ float:left; border-left:solid 1px #dddddd; height: 30px; padding-left: 20px;}
.top_catalog_right  a{background: url(../../images/201202/right_Arrowg.gif) no-repeat scroll left top  transparent; display: inline-block; height: 0 !important; overflow: hidden; z-index: 500;padding-top:15px;position: relative;top: 8px;}
.top_catalog_right  a:hover{background: url(../../images/201202/right_Arrow.gif) no-repeat scroll left top  transparent;}