BODY,TD,INPUT,SELECT {
	FONT-SIZE: $font$
}

A {
	COLOR: #000000
}.DlgBody {
	background-color: #9BAABF;
}
.list_td_title {
	color: #ffffff; 
	height: 17px; 
	font-family: "宋体"; 
	font-size: 9pt; 
	font-weight: bold; 
	border: #999999; 
	border-style: outset; 
	border-top-width: 1px; 
	border-right-width: 1px;
	border-bottom-width: 1px; 
	border-left-width: 1px; 	background-color: #609ceb;
	text-align: center
	}
.list_td_context {
	background-color: #ffffff; cellspacing:"0" ; cellpadding:"0"; height:20px
}
.list_td_context1 {
	background-color: #acd7f7; cellspacing:"0" ; cellpadding:"0"; height:20px
}
.list_td_context2 {
	background-color: #EEEEEE; cellspacing:"0" ; cellpadding:"0"; height:20px
}
.list_td_context3 {
	background-color: #A6DEFF; cellspacing:"0" ; cellpadding:"0"; height:20px
}

BODY.clsMainBody { 
	background-color : #85D2FF ;
	MARGIN-LEFT: 10px; margin-top: 0 px; 
	width: 100%; height: 100%; 
	SCROLLBAR-FACE-COLOR: #85d2ff; //滚动条凸出部分的颜色
	SCROLLBAR-HIGHLIGHT-COLOR: #0A00D1;//滚动条空白部分的颜色
	SCROLLBAR-SHADOW-COLOR: #FF7417;//立体滚动条阴影的颜色
	SCROLLBAR-3DLIGHT-COLOR: #425D7F;//滚动条亮边的颜色
	SCROLLBAR-ARROW-COLOR: #5DE8FF;//上下按钮上三角箭头的颜色
	SCROLLBAR-TRACK-COLOR: #85d2ff;//滚动条的背景颜色
	SCROLLBAR-DARKSHADOW-COLOR: #0A00D1;//滚动条强阴影的颜色
	SCROLLBAR-BASE-COLOR: #85d2ff；//滚动条的基本颜色
}


