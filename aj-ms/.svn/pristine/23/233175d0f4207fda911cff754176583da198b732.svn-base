package com.frame.ifpr.exception;

/**
 * 公共异常
 * 
 * @author lishun
 * 
 */
public class CommonException extends BaseException {
	private static final long serialVersionUID = 1L;

	public CommonException() {
		super();
	}

	public CommonException(String errMessage, Throwable e) {
		super(errMessage, e);
	}

	public CommonException(String errMessage) {
		super(errMessage);
	}

	public CommonException(Throwable errMessage) {
		super(errMessage);
	}

}
