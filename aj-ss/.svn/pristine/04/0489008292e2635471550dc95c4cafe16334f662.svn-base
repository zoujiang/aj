    float: left;
    width:65%;
}
.wy-number .wy-number-xx .wy-xx-sr li .wy-xx-mc{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: right;
}
.wy-number .wy-number-xx .wy-xx-sr li .wy-xx-xx{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: left;
    margin-left: 20px;
}
.wy-number .wy-number-xx .wy-xx-sr02{
    float: left;
