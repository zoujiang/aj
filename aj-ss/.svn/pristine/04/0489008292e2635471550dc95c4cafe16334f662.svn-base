}
.wy-number .wy-number-xx .wy-xx-sr02 li .wy-xx-mc{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: right;
}
.wy-number .wy-number-xx .wy-xx-sr02 li .wy-xx-xx{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: left;
    margin-left: 20px;
}
/* 成绩信息 计数器 */
.wy-number .wy-number-xx .wy-xx-cjcx{
    width:80%;
    margin:30px auto;
    overflow: hidden;

}
.wy-number .wy-number-xx .wy-xx-cjcx ul{
    float: left;
    width:50%;
}
.wy-number .wy-number-xx .wy-xx-cjcx ul li .wy-cx-cj{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: right;
}
.wy-number .wy-number-xx .wy-xx-cjcx ul li .wy-cx-cj02{
    display: inline-block;
    height: 60px;
    line-height: 60px;
    color: #444;
    text-align: left;
    margin-left: 20px;
}
.wy-number .wy-number-xx .wy-xx-zp{
    width:80%;
    margin: 0 auto;
}
.wy-number .wy-number-xx .wy-xx-zp p a{
    color:#0000FF;
    display: inline-block;
}
.wy-number .wy-number-xx .wy-xx-zp p{
    display: inline-block;
    height: 40px;
}
.wy-number .wy-number-xx .wy-xx-zp p a:hover{
    border-bottom: 1px solid #0000FF;
}
.wy-number .wy-number-xx .wy-xx-zp .wy-k-img{
    margin-top:30px;
}










